<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="istic.m2.ila.firefighterapp.activitiy.MapActivity"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

        <FrameLayout
            android:id="@+id/mapFragmentParent"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:id="@+id/mapFragment"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </FrameLayout>

            <fragment
                android:id="@+id/holder_fragment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:name="istic.m2.ila.firefighterapp.fragment.map.intervention.FragmentHolder"
                android:layout_gravity="bottom|left"
                android:layout_marginRight="90px">
            </fragment>

        </FrameLayout>


        <FrameLayout
            android:id="@+id/listViewFragment"
            android:layout_width="1100dp"
            android:layout_height="1000dp"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">
            </LinearLayout>
        </FrameLayout>
</FrameLayout>
