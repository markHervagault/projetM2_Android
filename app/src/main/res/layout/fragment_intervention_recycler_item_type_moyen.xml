<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/spaceTabMoyTop"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:height="1dp"
        android:visibility="gone"/>
<LinearLayout
    android:id="@+id/linLayTabMoy"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal" >


    <TextView
        android:id="@+id/expand_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dp"
        android:width="70dp"
        android:height="70dp"
        android:clickable="true"/>

    <TextView
        android:id="@+id/tvLabelMoyen"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dp"
        android:width="70dp"
        android:height="70dp"
        />

    <TextView
        android:id="@+id/tvEtatMoyen"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dp"
        android:width="120dp"
        android:height="70dp"
        />

    <TextView
        android:id="@+id/tvCrmOrNot"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dp"
        android:textColor="@android:color/black"
        android:text="CRM"
        android:width="70dp"
        android:height="50dp"

        />

    <TextView
        android:id="@+id/tvHeureDemande"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dp"
        android:width="120dp"
        android:text="..."
        android:height="70dp"

        />


    <TextView
        android:id="@+id/tvHeureValidationRefus"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dp"
        android:width="120dp"
        android:height="70dp"

        android:text="..."
        />


    <TextView
        android:id="@+id/tvHeureEngagement"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dp"
        android:width="120dp"
        android:height="70dp"

        android:text="..."
        />

    <TextView
        android:id="@+id/tvHeureLiberation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dp"
        android:width="120dp"
        android:height="70dp"

        android:text="..."
        />
</LinearLayout>
    <TextView
        android:id="@+id/spaceTabMoyBottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:height="1dp"
       />
</LinearLayout>